{
  "name": "adventure",
  "description": "An adventure looking for a hero!",
  "version": "3.5.5",
  "skill": {
    "namespace": "adventure",
    "upgradeIgnoreList": [
      "globby"
    ]
  },
  "homepage": "https://github.com/sprucelabsai/spruce-adventure-skill",
  "bugs": {
    "url": "https://github.com/sprucelabsai/spruce-adventure-skill/issues"
  },
  "keywords": [],
  "scripts": {
    "boot": "node build/index",
    "boot.local": "node -r ts-node/register -r tsconfig-paths/register ./src/index",
    "build": "yarn build.dev",
    "build.ci": "yarn build.tsc && yarn build.resolve-paths && yarn lint",
    "build.copy-files": "mkdir -p build && rsync -avzq --exclude='*.ts' ./src/ ./build/",
    "build.dev": "yarn build.tsc --sourceMap ; yarn resolve-paths.lint",
    "build.dist": "yarn build.tsc && yarn resolve-paths.lint",
    "build.resolve-paths": "resolve-path-aliases --target build --patterns '**/*.js,**/*.d.ts'",
    "build.tsc": "yarn build.copy-files && tsc",
    "clean": "yarn clean.build",
    "clean.all": "yarn clean.dependencies && yarn clean.build",
    "clean.build": "rm -rf build/",
    "clean.dependencies": "rm -rf node_modules/ package-lock.json yarn.lock",
    "fix.lint": "eslint --fix --cache '**/*.ts'",
    "health": "yarn boot --health",
    "health.local": "yarn boot.local --health",
    "heroku-postbuild": "yarn build.dev",
    "lint": "eslint --cache '**/*.ts'",
    "lint.tsc": "tsc -p . --noEmit",
    "rebuild": "yarn clean.all && yarn && yarn build.dev",
    "release": "semantic-release",
    "resolve-paths.lint": "yarn build.resolve-paths ; yarn lint",
    "polish": "heartwood-polish",
    "post.watch.build": "yarn build.copy-files && yarn build.resolve-paths",
    "test": "jest",
    "upgrade.packages": "yarn-upgrade-all && rm -f yarn.lock ; yarn ; yarn fix.lint ; true",
    "upgrade.packages.all": "yarn install && yarn upgrade.packages",
    "upgrade.packages.test": "yarn upgrade.packages.all && yarn lint && yarn build.dev && yarn test",
    "watch.build.dev": "tsc-watch --sourceMap --onCompilationComplete 'yarn post.watch.build'",
    "watch.lint": "concurrently 'yarn lint' \"chokidar 'src/**/*' -c 'yarn lint.tsc'\"",
    "watch.rebuild": "yarn clean.all && yarn && yarn watch.build.dev",
    "watch.tsc": "tsc -w",
    "update.dependencies": "yarn clean.dependencies && yarn"
  },
  "dependencies": {
    "@sprucelabs/calendar-utils": "^27.1.0",
    "@sprucelabs/data-stores": "^18.1.49",
    "@sprucelabs/error": "^5.0.512",
    "@sprucelabs/heartwood-view-controllers": "^90.10.11",
    "@sprucelabs/mercury-client": "^31.0.200",
    "@sprucelabs/mercury-core-events": "^12.2.14",
    "@sprucelabs/mercury-types": "^35.0.143",
    "@sprucelabs/schema": "^28.5.10",
    "@sprucelabs/spruce-core-schemas": "^34.2.101",
    "@sprucelabs/spruce-event-plugin": "^49.0.38",
    "@sprucelabs/spruce-event-utils": "^27.0.174",
    "@sprucelabs/spruce-image-utils": "^5.0.4",
    "@sprucelabs/spruce-invite-utils": "^5.0.47",
    "@sprucelabs/spruce-skill-booter": "^49.0.38",
    "@sprucelabs/spruce-skill-utils": "^26.4.78",
    "@sprucelabs/spruce-store-plugin": "^49.0.38",
    "@sprucelabs/spruce-view-plugin": "^34.0.48",
    "dotenv": "^16.0.2",
    "globby": "^11.0.4"
  },
  "devDependencies": {
    "@sprucelabs/heartwood-polish": "^2.1.116",
    "@sprucelabs/jest-json-reporter": "^6.0.472",
    "@sprucelabs/jest-sheets-reporter": "^1.3.102",
    "@sprucelabs/resolve-path-aliases": "^1.1.99",
    "@sprucelabs/semantic-release": "^4.0.8",
    "@sprucelabs/spruce-test-fixtures": "^49.0.38",
    "@sprucelabs/test": "^7.7.360",
    "@sprucelabs/test-utils": "^3.2.2",
    "@types/node": "^18.7.17",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.23.1",
    "eslint-config-spruce": "^10.11.4",
    "jest": "^29.0.3",
    "jest-circus": "^29.0.3",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "ts-watch": "^1.0.8",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  },
  "resolutions": {
    "**/terser-webpack-plugin": "5.1.4"
  },
  "engines": {
    "node": ">=12 <17",
    "yarn": "1.x"
  },
  "jest": {
    "testRunner": "jest-circus/runner",
    "maxWorkers": 4,
    "testTimeout": 120000,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/tmp/",
      "<rootDir>/src/",
      "<rootDir>/node_modules/",
      "<rootDir>/build/__tests__/testDirsAndFiles/"
    ],
    "reporters": [
      "default",
      [
        "@sprucelabs/jest-sheets-reporter",
        {
          "sheetId": "1DoWM7mYgGDa-PJXptLomrZmbIBZiuxS1YiAsRXU2Gm0",
          "worksheetId": 1262763347,
          "testMap": {
            "postFormRendersExpectedFields": "B1",
            "submittingFormRendersConfirm": "B2",
            "cancellingSaveDoesNotSave": "B3",
            "confirmingSubmitPostsAdventure": "B4",
            "rendersAlertWhenFailingToSave": "B5",
            "showsCurrentCardAfterSaving": "B6",
            "postingAdventureHidesPostCard": "B7",
            "assertCurrentCardRendersCancelButton": "B8",
            "onlyReturnsYoursWithoutFriendsInTheMiddle": "B22",
            "currentCardHasProperAdventure": "B12",
            "clickingCancelRendersConfirm": "B9",
            "postingAdventureSavesUnderCurrentPerson": "B15",
            "redirectToListIfLoggedIn": "B37",
            "listSkillViewRequiresLogin": "B38",
            "rendersInviteButton": "B42",
            "rendersRowForEachFriend": "B45",
            "onlyReturnsYoursWithoutFriends": "B52",
            "dropsInAvatar": "B51",
            "confirmingCancelEmitsCancelEvent": "B11",
            "doesNotDeleteOtherPeoples": "B23",
            "decliningCancelDoesNotEmit": "B10",
            "clickInviteRedirectsToInvite": "B43",
            "hasConnectSkillView": "B44",
            "listFriendsHonorsSourceEmitting": "B46",
            "rendersExpectedButtonsForWhomeverSent": "B47",
            "acceptsConnectionIfPassedThroughArgs": "B58",
            "setsPersonToSourceBasedOnId": "B59",
            "hasCReateConnectionListener": "B60",
            "redirectsToListEvenIfOwnAdventureIsNotFirst": "B19"
