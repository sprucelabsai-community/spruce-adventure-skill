{
  "name": "adventure",
  "description": "An adventure looking for a hero!",
  "version": "10.0.103",
  "skill": {
    "namespace": "adventure",
    "upgradeIgnoreList": [],
    "version": "v2022_09_09"
  },
  "homepage": "https://github.com/sprucelabsai/spruce-adventure-skill",
  "bugs": {
    "url": "https://github.com/sprucelabsai/spruce-adventure-skill/issues"
  },
  "keywords": [],
  "scripts": {
    "boot": "node build/index",
    "boot.local": "node -r ts-node/register -r tsconfig-paths/register ./src/index",
    "build": "yarn build.dev",
    "build.ci": "yarn run build.tsc && yarn run build.resolve-paths && yarn run lint",
    "build.copy-files": "mkdir -p build && rsync -avzq --exclude='*.ts' ./src/ ./build/",
    "build.dev": "yarn run build.tsc --sourceMap ; yarn run resolve-paths.lint",
    "build.dist": "yarn build.tsc && yarn resolve-paths.lint",
    "build.resolve-paths": "resolve-path-aliases --target build --patterns '**/*.js,**/*.d.ts'",
    "build.tsc": "yarn run build.copy-files && tsc",
    "clean": "yarn run clean.build",
    "clean.all": "yarn run clean.dependencies && yarn run clean.build",
    "clean.build": "rm -rf build/",
    "clean.dependencies": "rm -rf node_modules/ package-lock.json yarn.lock",
    "fix.lint": "eslint --fix --cache '**/*.ts'",
    "health": "yarn boot --health",
    "health.local": "yarn boot.local --health",
    "heroku-postbuild": "yarn build.dev",
    "lint": "eslint --cache '**/*.ts'",
    "lint.tsc": "tsc -p . --noEmit",
    "rebuild": "yarn run clean.all && yarn install && yarn run build.dev",
    "release": "semantic-release",
    "resolve-paths.lint": "yarn run build.resolve-paths ; yarn run lint",
    "polish": "heartwood-polish",
    "post.watch.build": "yarn run build.copy-files && yarn run build.resolve-paths",
    "test": "jest",
    "upgrade.packages": "yarn-upgrade-all && rm -f yarn.lock ; yarn ; yarn fix.lint ; true",
    "upgrade.packages.all": "yarn install && yarn upgrade.packages",
    "upgrade.packages.test": "yarn upgrade.packages.all && yarn lint && yarn build.dev && yarn test",
    "watch.build.dev": "tsc-watch --sourceMap --onCompilationComplete 'yarn run post.watch.build'",
    "watch.rebuild": "yarn run clean.all && yarn install && yarn run watch.build.dev",
    "watch.tsc": "tsc -w",
    "update.dependencies": "yarn run clean.dependencies && yarn"
  },
  "dependencies": {
    "@sprucelabs/calendar-utils": "^43.0.3",
    "@sprucelabs/data-stores": "^29.0.3",
    "@sprucelabs/error": "^7.0.1",
    "@sprucelabs/globby": "^2.0.504",
    "@sprucelabs/heartwood-view-controllers": "^121.1.7",
    "@sprucelabs/mercury-client": "^43.0.3",
    "@sprucelabs/mercury-core-events": "^27.0.2",
    "@sprucelabs/mercury-types": "^48.0.3",
    "@sprucelabs/schema": "^32.0.3",
    "@sprucelabs/spruce-core-schemas": "^41.0.3",
    "@sprucelabs/spruce-event-plugin": "^68.0.2",
    "@sprucelabs/spruce-event-utils": "^41.0.3",
    "@sprucelabs/spruce-form-utils": "^21.0.103",
    "@sprucelabs/spruce-image-utils": "^12.0.38",
    "@sprucelabs/spruce-invite-utils": "^12.0.100",
    "@sprucelabs/spruce-permission-plugin": "^38.0.2",
    "@sprucelabs/spruce-skill-booter": "^68.0.2",
    "@sprucelabs/spruce-skill-utils": "^32.0.3",
    "@sprucelabs/spruce-store-plugin": "^68.0.2",
    "@sprucelabs/spruce-view-plugin": "^53.0.3",
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@sprucelabs/heartwood-polish": "^7.2.134",
    "@sprucelabs/jest-json-reporter": "^9.0.2",
    "@sprucelabs/resolve-path-aliases": "^3.0.1",
    "@sprucelabs/semantic-release": "^5.0.2",
    "@sprucelabs/spruce-test-fixtures": "^68.0.2",
    "@sprucelabs/test": "^10.0.0",
    "@sprucelabs/test-utils": "^6.0.3",
    "@types/node": "^24.0.1",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9.28.0",
    "eslint-config-spruce": "^11.2.26",
    "jest": "^30.0.0",
    "jest-circus": "^30.0.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "ts-watch": "^1.0.8",
    "tsc-watch": "^7.1.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "resolutions": {
    "**/terser-webpack-plugin": "5.1.4"
  },
  "engines": {
    "yarn": "1.x"
  },
  "jest": {
    "testRunner": "jest-circus/runner",
    "maxWorkers": 4,
    "testTimeout": 120000,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/tmp/",
      "<rootDir>/src/",
      "<rootDir>/node_modules/",
      "<rootDir>/build/__tests__/testDirsAndFiles/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ],
    "moduleNameMapper": {
      "^#spruce/(.*)$": "<rootDir>/build/.spruce/$1"
    }
  }
}
