{
  "name": "adventure",
  "description": "An adventure looking for a hero!",
  "version": "1.3.41",
  "skill": {
    "namespace": "adventure",
    "upgradeIgnoreList": [
      "globby"
    ]
  },
  "homepage": "https://github.com/sprucelabsai/spruce-adventure-skill",
  "bugs": {
    "url": "https://github.com/sprucelabsai/spruce-adventure-skill/issues"
  },
  "keywords": [],
  "scripts": {
    "boot": "node build/index",
    "boot.local": "node -r ts-node/register -r tsconfig-paths/register ./src/index",
    "build": "yarn build.dev",
    "build.ci": "yarn build.tsc && yarn build.resolve-paths && yarn lint",
    "build.copy-files": "mkdir -p build && rsync -avzq --exclude='*.ts' ./src/ ./build/",
    "build.dev": "yarn build.tsc --sourceMap ; yarn resolve-paths.lint",
    "build.dist": "yarn build.tsc && yarn resolve-paths.lint",
    "build.resolve-paths": "resolve-path-aliases --target build --patterns '**/*.js,**/*.d.ts'",
    "build.tsc": "yarn build.copy-files && tsc",
    "clean": "yarn clean.build",
    "clean.all": "yarn clean.dependencies && yarn clean.build",
    "clean.build": "rm -rf build/",
    "clean.dependencies": "rm -rf node_modules/ package-lock.json yarn.lock",
    "fix.lint": "eslint --fix --cache '**/*.ts'",
    "health": "yarn boot --health",
    "health.local": "yarn boot.local --health",
    "heroku-postbuild": "yarn build.dev",
    "lint": "eslint --cache '**/*.ts'",
    "lint.tsc": "tsc -p . --noEmit",
    "rebuild": "yarn clean.all && yarn && yarn build.dev",
    "release": "semantic-release",
    "resolve-paths.lint": "yarn build.resolve-paths ; yarn lint",
    "polish": "heartwood-polish",
    "post.watch.build": "yarn build.copy-files && yarn build.resolve-paths",
    "test": "jest",
    "upgrade.packages": "yarn-upgrade-all && rm -f yarn.lock ; yarn ; yarn fix.lint ; true",
    "upgrade.packages.all": "yarn install && yarn upgrade.packages",
    "upgrade.packages.test": "yarn upgrade.packages.all && yarn lint && yarn build.dev && yarn test",
    "watch.build.dev": "tsc-watch --sourceMap --onCompilationComplete 'yarn post.watch.build'",
    "watch.lint": "concurrently 'yarn lint' \"chokidar 'src/**/*' -c 'yarn lint.tsc'\"",
    "watch.rebuild": "yarn clean.all && yarn && yarn watch.build.dev",
    "watch.tsc": "tsc -w",
    "update.dependencies": "yarn clean.dependencies && yarn"
  },
  "dependencies": {
    "@sprucelabs/error": "^5.0.496",
    "@sprucelabs/heartwood-view-controllers": "^88.0.23",
    "@sprucelabs/mercury-client": "^31.0.160",
    "@sprucelabs/mercury-core-events": "^12.0.161",
    "@sprucelabs/mercury-types": "^35.0.109",
    "@sprucelabs/schema": "^28.4.25",
    "@sprucelabs/spruce-core-schemas": "^34.2.67",
    "@sprucelabs/spruce-event-plugin": "^47.0.21",
    "@sprucelabs/spruce-event-utils": "^27.0.138",
    "@sprucelabs/spruce-skill-booter": "^47.0.21",
    "@sprucelabs/spruce-skill-utils": "^26.4.41",
    "@sprucelabs/spruce-view-plugin": "^32.0.22",
    "dotenv": "^16.0.1",
    "globby": "^11.0.4"
  },
  "devDependencies": {
    "@sprucelabs/heartwood-polish": "^2.1.48",
    "@sprucelabs/jest-json-reporter": "^6.0.454",
    "@sprucelabs/resolve-path-aliases": "^1.1.83",
    "@sprucelabs/semantic-release": "^4.0.8",
    "@sprucelabs/spruce-test-fixtures": "^47.0.21",
    "@sprucelabs/test": "^7.7.346",
    "@sprucelabs/test-utils": "^3.1.65",
    "@types/node": "^18.7.13",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.3.0",
    "eslint": "^8.22.0",
    "eslint-config-spruce": "^10.11.3",
    "jest": "^28.1.3",
    "jest-circus": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "ts-watch": "^1.0.8",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "**/terser-webpack-plugin": "5.1.4"
  },
  "engines": {
    "node": ">=12 <17",
    "yarn": "1.x"
  },
  "jest": {
    "testRunner": "jest-circus/runner",
    "maxWorkers": 4,
    "testTimeout": 120000,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/tmp/",
      "<rootDir>/src/",
      "<rootDir>/node_modules/",
      "<rootDir>/build/__tests__/testDirsAndFiles/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ],
    "moduleNameMapper": {
      "^#spruce/(.*)$": "<rootDir>/build/.spruce/$1"
    }
  }
}
